# Server
PORT=3001
NODE_ENV=development

# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://user:password@host:5432/database

# JWT
JWT_SECRET=change-this-in-production-use-random-64-chars

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:5175

# IntaSend API (Wallet-as-a-Service)
INTASEND_TEST=true
INTASEND_PUBLISHABLE_KEY=your_publishable_key_here
INTASEND_SECRET_KEY=your_secret_key_here
INTASEND_BASE_URL=https://sandbox.intasend.com/api/v1
